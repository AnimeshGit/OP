                        <form class="form-signup" action="/admin_register" method="post" role="form">
                            <div class="row">
                                <div class="col-sm-6">
                                    <div class="append-icon">
                                        <input type="text" name="adminFirstName" id="adminFirstName" class="form-control form-white firstname" placeholder="First Name"  autofocus required>
